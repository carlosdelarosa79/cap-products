        "ToCategory": {
          "@mandatory": true,
          "type": "cds.String"
        },
        "ToDimensionUnit_Id": {
          "type": "cds.String"
        },
        "SalesData": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "CatalogService.SalesData",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "Supplier_Id": {
          "type": "cds.String"
        },
        "Reviews": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "CatalogService.Reviews",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "Rating": {
          "@Core.Computed": true,
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 2
        },
        "StockAvailability": {
          "@Core.Computed": true,
          "type": "cds.Integer"
        },
        "ToStockAvailibity": {
          "type": "cds.Association",
          "target": "CatalogService.StockAvailability",
          "on": [
            {
              "ref": [
                "ToStockAvailibity",
                "ID"
              ]
            },
            "=",
