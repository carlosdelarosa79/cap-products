              "ref": [
                "UnitOfMeasure_Id"
              ],
              "as": "ToUnitOfMeasure"
            },
            {
              "ref": [
                "Currency_Id"
              ],
              "as": "ToCurrency"
            },
            {
              "ref": [
                "Category_Id"
              ],
              "as": "ToCategory"
            },
            {
              "ref": [
                "DimensionUnit_Id"
              ],
              "as": "ToDimensionUnit_Id"
            },
            {
              "ref": [
                "SalesData"
              ]
            },
            {
              "ref": [
                "Supplier_Id"
              ]
            },
            {
              "ref": [
                "Reviews"
              ]
            },
            {
              "ref": [
                "Rating"
              ]
            },
            {
              "ref": [
                "StockAvailability"
              ]
            },
            {
              "ref": [
                "ToStockAvailibity"
              ]
            }
          ]
        }
      },
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "PoductName": {
          "@mandatory": true,
          "localized": true,
          "type": "cds.String",
          "notNull": true
        },
        "Description": {
          "@mandatory": true,
          "localized": true,
          "type": "cds.String"
        },
        "ImageUrl": {
          "type": "cds.String"
        },
        "ReleaseDate": {
          "type": "cds.DateTime",
          "default": {
            "ref": [
              "$now"
            ]
          }
        },
        "DiscontinuedDate": {
          "type": "cds.DateTime"
        },
        "Price": {
          "@mandatory": true,
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 2
        },
        "Height": {
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 2
        },
        "Width": {
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 2
        },
        "Depth": {
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 2
        },
        "Quantity": {
          "@mandatory": true,
          "@assert.range": [
            0,
            20
          ],
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 2
        },
        "ToUnitOfMeasure": {
          "@mandatory": true,
          "type": "cds.String"
        },
