              "as": "PoductName"
            },
            {
              "ref": [
                "Description"
              ]
            },
            {
              "ref": [
                "ImageUrl"
              ]
            },
            {
              "ref": [
                "ReleaseDate"
              ]
            },
            {
              "ref": [
                "DiscontinuedDate"
              ]
            },
            {
              "ref": [
                "Price"
              ]
            },
            {
              "ref": [
                "Height"
              ]
            },
            {
              "ref": [
                "Width"
              ]
            },
