          "from": {
            "ref": [
              "com.logali.materials.Products"
            ]
          },
          "mixin": {
            "ToStockAvailibity": {
              "type": "cds.Association",
              "target": "com.logali.materials.StockAvailability",
              "on": [
                {
                  "ref": [
                    "ToStockAvailibity",
                    "ID"
                  ]
